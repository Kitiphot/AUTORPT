    function getSubmitRequest(filter) {
        if (!filter) {
            filter = {
                selectPickStartDate: submitCriteria.selectPickStartDate,
                selectPickEndDate: submitCriteria.selectPickEndDate,
                selectPackStartDate: submitCriteria.selectPackStartDate,
                selectPackEndDate: submitCriteria.selectPickEndDate,
                selectCustomerName: null
            };
        }
        else {
            filter = {
                selectPickStartDate: moment(filter.selectPickStartDate).format('YYYY/MM/DD') ?? submitCriteria.selectPickStartDate,
                selectPickEndDate: moment(filter.selectPickEndDate).format('YYYY/MM/DD') ?? submitCriteria.selectPickEndDate,
                //selectPackStartDate: moment(filter.selectPackStartDate).isValid()?:moment(filter.selectPackStartDate).format('YYYY/MM/DD'):submitCriteria.selectPackStartDate,

                selectPackStartDate: moment(filter.selectPackStartDate).format('YYYY/MM/DD') ?? submitCriteria.selectPackStartDate,
                selectPackEndDate: moment(filter.selectPackEndDate).format('YYYY/MM/DD') ?? submitCriteria.selectPackEndDate,
                CustomerName: filter.CustomerName ?? null
            };
        }

        $('#ModalTransactionDate').text(moment(filter.orderStartDate).format('DD/MM/YYYY') + '-' + moment(filter.orderEndDate).format('DD/MM/YYYY'));
        return {
            ...submitCriteria,
            selectCustomerName: filter.CustomerName,
            selectPickStartDate: filter.selectPickStartDate,
            selectPickEndDate: filter.selectPickEndDate,
            selectPackStartDate: filter.selectPackStartDate,
            selectPackEndDate: filter.selectPackEndDate
        }
    }

