                                scaleLabel: {
                                    display: true,
                                    labelString: ""
                                },
                                ticks: {
                                    autoSkip: false,
                                    maxRotation: 90,
                                    minRotation: 0,
                                    callback: function (value, index, values) {
                                        return value.substring(0, 2);
                                    }
                                },
                                maxBarThickness: 60
                            }],
                            yAxes: [{
                                stacked: true,
                                ticks: {
                                    beginAtZero: true,
                                },
