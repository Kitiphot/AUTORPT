@model SCG.ARS.BOI.WEB.Models.ListMiscDataSelectionViewModel
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">เงื่อนไขการค้นหา</h4>
                <div class="form-row">
                    <div class="col-lg-12 col-xl-12 col-md-12 col-sm-12">
                        <div class="form-row">
                            <div class="form-group col-xl-2 col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group row">
                                    <label for="daterange-01">Pick Date</label>
                                    <div class="col-sm-12">
                                        <input type="button" id="daterange-01" class="btn btn-default dropdown-toggle md-btn-flat cbFormat" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-xl-2 col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group row">
                                    <label for="daterange-02">Pack Date</label>
                                    <div class="col-sm-12">
                                        <input type="button" id="daterange-02" class="btn btn-default dropdown-toggle md-btn-flat cbFormat" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-xl-2 col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group row">
                                    <label for="cboDC">Location</label>
                                    <div class="col-sm-12">
                                        @Html.DropDownListFor(m => m.Location, Enumerable.Empty<SelectListItem>(), null, new { @id = "cboDC", @name = "DC[]", multiple = "multiple", @style = "width: 100%" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-xl-2 col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group row">
                                    <label for="cboCustomer">Customers</label>
                                    <div class="col-sm-12">
                                        @Html.DropDownListFor(m => m.Customer, Enumerable.Empty<SelectListItem>(), null, new { @id = "cboCustomer", @name = "customer[]", multiple = "multiple", @style = "width: 100%" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-xl-2 col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group row">
                                    <label for="cboProductGroup">Product Group</label>
                                    <div class="col-sm-12">
                                        @Html.DropDownListFor(m => m.Productgroup, Enumerable.Empty<SelectListItem>(), null, new { @id = "cboProductGroup", @name = "productgroup[]", multiple = "multiple", @style = "width: 100%" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group col-xl-2 col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group col mt-2">
                                    <div class="wrapper">
                                        <div id="search-button">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*<div class="col-lg-12 col-xl-2 col-md-12 col-sm-12">
                        <div class="form-group col mt-2">
                            <div class="wrapper">
                                <div id="search-button">
                                </div>
                            </div>
                        </div>
                    </div>*@
                </div>
            </div>
        </div>
    </div>
</div>