                    <div class="form-group col-lg-3" id="cbBusiness_Raw">
                        <label for="cboBusiness_Raw">Business</label>
                        @Html.DropDownListFor(m => m.Business, Enumerable.Empty<SelectListItem>(), null, new { @id = "cboBusiness_Raw", multiple = "multiple", @style = "width: 100%"  })
                    </div>
                    <div class="form-group col-lg-3" id="cbMatfrg_Raw">
