                data: request,
                dataType: 'Json'
            }).done(function (response) {
                var ctx = document.getElementById("barChartPickByCustomer").getContext('2d');
                var myChart = new Chart(ctx, {
                    type: 'bar',
                    data: response,
                    options: {
                        scales: {
                            xAxes: [{
                                barPercentage: 0.6,
