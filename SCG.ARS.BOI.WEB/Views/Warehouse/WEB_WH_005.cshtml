                            display: true,
                            position: "bottom"
                        },
                        title: {
                            display: true,
                            text: ""
                        }
                    }
                });
                var canvas = document.getElementById('barChartPickByCustomer');
                canvas.onclick = function (evt) {
                    var activePoints = myChart.getElementsAtEvent(evt);
                    if (activePoints[0]) {

                        // x label
                        var chartData = activePoints[0]['_chart'].config.data;
                        var idx = activePoints[0]['_index'];
                        var pickDate = chartData.labels[idx];

                        var activePoint = myChart.getElementAtEvent(evt)[0];
                        var data = activePoint._chart.data;
                        var datasetIndex = activePoint._datasetIndex;
                        var CustomerLabel = data.datasets[datasetIndex].label;

                        console.log('CustomerName');
                        console.log(CustomerLabel);
                        console.log('Pick Date');
                        console.log(pickDate);
                        var pickDate = moment(pickDate, 'DD/MM/YYYY').format('YYYY/MM/DD');
                    }
                    genTableModal({ selectPickStartDate: pickDate, selectPickEndDate: pickDate, selectPackStartDate: submitCriteria.selectPackStartDate, selectPackEndDate: submitCriteria.selectPackEndDate, CustomerName: CustomerLabel });
                    DrawTableDetail(getRequest(CustomerLabel, pickDate,null));
                };

            }).fail(function (error) {
                console.log(error);
            }).always(function () {
                $('#SectionBarChartPickByCustomer').hide();
                //console.log( "finished" );
            });


            $('#SectionBarChartPackByCustomer').show();
            $('#barChartPackByCustomer').remove();
            $('#barChartPackByCustomerContainer').append('<canvas id="barChartPackByCustomer" class="chartCanvas"><canvas>');


            $.ajax({
                url: '@Url.Action("RPTCDC005_PackByCustomer", "Warehouse")',
                type: 'POST',
                data: request,
                dataType: 'Json'
            }).done(function (response) {
                var ctx = document.getElementById("barChartPackByCustomer").getContext('2d');
                var myChart = new Chart(ctx, {
                    type: 'bar',
                    data: response,
                    options: {
                        scales: {
                            xAxes: [{
                                barPercentage: 0.6,
                                stacked: true,
                                gridLines: {
                                    display: false,
                                }
                                ,
                                scaleLabel: {
                                    display: true,
                                    labelString: ""
                                },
                                ticks: {
                                    autoSkip: false,
                                    maxRotation: 90,
                                    minRotation: 0,
                                    callback: function (value, index, values) {
                                        return value.substring(0, 2);
                                    }
                                },
                                maxBarThickness: 60
                            }],
                            yAxes: [{
                                stacked: true,
                                ticks: {
                                    beginAtZero: true,
                                },
                                type: 'linear',
                                scaleLabel: {
                                    display: true,
                                    labelString: "กล่อง"
                                }
                            }]
