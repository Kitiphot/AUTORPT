                    <div class="form-group col-lg-3" id="cbPlannerName_Raw">
                        <label for="cboPlannerName_Raw">Planner Name</label>
                        @Html.DropDownListFor(m => m.Planner_Name, Enumerable.Empty<SelectListItem>(), null, new { @id = "cboPlannerName_Raw", multiple = "multiple", @style = "width: 100%" })
